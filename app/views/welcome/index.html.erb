<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>
<%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %>

<% if user_signed_in? %>
  <div class="avatar">
    <img height="24" width="24" src="<%= current_user.image_url %>"/>
    <br/>
    <%= current_user.name %>
  </div>
<% else %>
  Not signed in
<% end %>
